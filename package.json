{
  "name": "eslint-plugin-dependency-relation",
  "version": "0.1.3",
  "description": "Import relation linting rule for ESLint.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/YutamaKotaro/eslint-plugin-dependencies-relation"
  },
  "bugs": {
    "url": "https://github.com/YutamaKotaro/eslint-plugin-dependencies-relation/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "lint": "eslint src/**",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "eslint",
    "typescript",
    "plugin",
    "import",
    "dependency"
  ],
  "author": "yutamakotaro",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^7.2.10",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/experimental-utils": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "eslint-module-utils": "^2.6.0",
    "eslint-plugin-import": "*",
    "extract-comments": "^1.1.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/?(*.)+(spec).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  }
}
